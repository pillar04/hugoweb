@media(min-width:576px){.mobile-navigation{display:none}}@media(min-width:0px) and (max-width:400px){nav.desktop{padding:0 .25rem}}nav button,nav button:hover,nav button:active,nav button:focus{margin:0;padding:0;border:none;outline:none;background-color:transparent}body>nav{z-index:99;padding-top:1.2rem!important;padding-bottom:1.3rem!important}body>nav>.container{display:flex;flex-wrap:inherit;align-items:center;justify-content:space-between;margin:0 auto;width:100%;padding-left:12px;padding-right:12px;height:2rem}body>nav>.container a{padding:0;margin:0}body>nav>.container a .squeezeLogo{width:6vw;height:3vw;min-width:60px;min-height:30px;object-fit:contain}body>nav>.container a .backpage{width:120px;height:40px;display:inline-block;line-height:40px;font-weight:400;font-size:16px;color:#fff;border:0;padding:0;box-sizing:border-box;background:linear-gradient(84deg,rgba(0,82,218,.8) 0%,rgba(96,165,255,.8) 100%)!important;border-radius:62px;font-weight:600;border:none;text-align:center}body>nav>.container a .backpage:hover{color:#fff;background:linear-gradient(84deg,#0052da 0%,#60a5ff 100%)!important}body>nav h1{margin-bottom:0;font-size:1.25rem;text-transform:uppercase}nav span{padding:.225rem 0}nav.desktop{background-color:var(--background-color)}nav .menu-active{border-bottom:5px var(--primary)solid}.sidebar{position:fixed;padding:1rem;background-color:var(--background-color);box-shadow:var(--card-box-shadow);min-width:100%}.sidebar button{position:absolute;top:var(--spacing);right:var(--spacing);width:1.5rem;height:1.5rem;border-radius:2rem;line-height:1}.sidebar h3,.sidebar a,.sidebar a:hover,.sidebar a:active,.sidebar a:focus{color:var(--secondary)}.mobile-navigation>li>button>svg{width:30px;height:30px}.sidebar svg,body>nav svg{height:1rem}#switch{display:none}@media(min-width:576px){.container-sm,.container{max-width:540px}}@media(min-width:768px){.container-md,.container-sm,.container{max-width:720px}}@media(min-width:992px){.navbar .container{max-width:960